		source = ```
				
				ADDCOLUMNS (
				    CALENDAR (
				        DATE( YEAR ( MIN ( BBDD[FECHA CREACION] ) ), 01, 01 ),
				        DATE( YEAR ( MAX ( BBDD[FECHA CREACION] ) ), 12, 31 )
				    ),
				    "FechaSK",       FORMAT ( [Date], "YYYYMMDD" ),
				    "A침o",           YEAR ( [Date] ),
				    "#Trimestre",    QUARTER ( [Date] ),
				    "#Mes",          MONTH ( [Date] ),
				    "#D칤a",          DAY ( [Date] ),
				    "Trimestre",     "T" & FORMAT ( [Date], "Q" ),
				    "Mes",           FORMAT ( [Date], "MMMM" ),
				    "MesCorto",      FORMAT ( [Date], "MMM" ),
				    "#D칤aSemana",    WEEKDAY ( [Date], 2 ),
				    "#SemanaA침o",    WEEKNUM ( [Date], 2 ),
				    "CierreSemana",  ( [Date] + 7 - WEEKDAY( [Date], 2 ) ),
				    "D칤a",           FORMAT ( [Date], "DDDD" ),
				    "D칤aCorto",      FORMAT ( [Date], "DDD" ),
				    "A침oTrimestre",  FORMAT ( [Date], "YYYY" ) & "/T" & FORMAT ( [Date], "Q" ),
				    "A침o#Mes",       FORMAT ( [Date], "YYYY/MM" ),
				    "A침oMesCorto",   FORMAT ( [Date], "YYYY/mmm" ),
				    "CierreMes",     EOMONTH( [Date], 0 ),
				
				    // 游댳 Nueva columna: Semana cronol칩gica (S01, S02, S03...)
